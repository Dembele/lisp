;;;№18. Определите предикат, проверяющий, является ли аргумент одноуровневым	;
;;;списком.									;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
(defun t18(input-list)
	(cond 	((null (car input-list)) t)
			((not (atom (car input-list))) nil)
			(t (t18 (cdr input-list)))
	)
)

