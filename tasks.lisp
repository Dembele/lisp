(t18 '((1 2) 3 4))		; NIL
(t18 '((1) 2 3 4))		; NIL
(t18 `(1 2 (3) 4))		; NIL

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
;;;№22. Определите функцию, которая обращает список (а b с) и разбивает его на	;
;;;уровни (((с) b) а).								;
;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
(defun t22 (input-list)
(cond 	((null (car input-list)) nil)
		(t 
		((lambda (elem result)
			(cons 
				(cons (car result) (cdr result)) (list elem)))
			(car input-list)
			(t22 (cdr input-list)))
		)
	)
)
